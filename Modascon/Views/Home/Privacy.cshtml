@using Entities.Models;
@model List<Product>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width_device-width, initial-scale=1.0" />
    <title>Document</title>
</head>
<body>
    @foreach (Product prd in Model)
    {
        if (prd != null)
        {
            <div>
                <h3>@prd.ProductName</h3>
                <p>@prd.Price</p>
            </div>
        }
        else
        {
            <p>Product is null.</p>
        }
    }

</body>