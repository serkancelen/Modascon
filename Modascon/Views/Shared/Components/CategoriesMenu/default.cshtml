@model IEnumerable<Category>

<div class="mt-3">
    <h6 class="lead bi bi-list">
        <strong><img src="~/images/toggle-icon.png">
        Kategoriler
        </strong>
    </h6>
</div>

<div class="my-4">
    <ul class="list-group">
        @foreach (var cat in Model)
        {
            <li class="list-group-item">
                <a asp-action="Index" asp-route-categoryId="@cat.CategoryId">
                    @cat.CategoryName
                </a>
            </li>
        }
    </ul>
</div>